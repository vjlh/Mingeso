pipeline{
  stages{
    stage('Git'){
      steps{
        git 'https://github.com/vjlh/Mingeso'
      }
    }
    stage('Install dependencies'){
      steps{
        sh 'npm run build'
        }
    }
    stage('Build'){
      steps{
        sh 'npm run build'
      }
    }
    stage('Test'){
      steps{
        sh 'npm test'
      }
    }
  }
}
